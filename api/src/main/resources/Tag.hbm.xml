<hibernate-mapping package="org.openmrs">
    <class name="Tag" table="tag_tag" batch-size="25">
        <id name="id" column="tag_id" unsaved-value="0">
            <generator class="native" />
        </id>
        <property name="tag" type="java.lang.String"
                  column="tag" length="255" not-null="true"/>
        <property name="object_uuid" type="java.lang.String"
                  column="object_uuid" length="38" not-null="true"/>
        <property name="object_type" type="java.lang.String"
                  column="object_type" length="255" not-null="true" />
        <many-to-one name="creator" class="User" column="creator" not-null="true"/>
        <property name="dateCreated" type="java.util.Date" column="date_created" length="19" not-null="true"/>
        <many-to-one name="changedBy" class="User" column="changedBy" />
        <property name="date_changed" type="java.util.Date" column="date_changed" length="19" />
        <property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true"/>
    </class>
</hibernate-mapping>